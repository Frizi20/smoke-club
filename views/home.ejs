    <script>
      const burger = document.querySelector(".burger");
      const navBurger = document.querySelector(".nav-burger");
      const body = document.querySelector("body");
      const mainBlurr = document.querySelector(".main-blurr");
      const sideNav = document.querySelector(".nav-links-container");
      const container = document.querySelector(".container");
      const navDetails = document.querySelector(".nav-details");
      const dropBoxes = Array.from(document.querySelectorAll(".drop-box"));
      const searchButton = document.querySelector('.src-btn-2');
      const closeSearchButton = document.querySelector('.close-src-btn');
      const searchActive = document.querySelector('.search-active');
      const secondDropBoxes = Array.from(
        document.querySelectorAll(".drop-box2")
      );

      const navLists = Array.from(document.querySelectorAll(".nav-links>li"));
      console.log(navLists);
      let scroll = true;

      //function

      function moveNav(){
        burger.classList.toggle("burger-active");
            body.classList.toggle("body-active");

            if (window.getComputedStyle(mainBlurr).display === "none") {
              mainBlurr.style.display = "block";
            } else {
              mainBlurr.style.display = "none";
            }

            scroll = !scroll;

            sideNav.style.width = "260px";
            sideNav.style.transition;
            container.classList.toggle("container-active");
            navDetails.classList.toggle("nav-details-active");
      }

      // TOGGLE BURGER ELEENT

      burger.addEventListener("click", (e) => {
          moveNav();
      });

      //Close the sidenav if the page is clicked except the sidenav

      document.addEventListener("click", function (e) {
        if (
          e.target.classList[0] === "main-blurr" ||
          e.target.classList[0] === "logo-name" ||
          e.target.classList[0] === "nav-details"
        ) {
          if (burger.classList[1] === "burger-active") {
            moveNav();
          }
        }
      });

      //Even Listener to deactivate the side nav,burger and all the same effects as when you press the burger to deactivate it

      window.addEventListener("resize", function () {
        // On screen change make the foother wrapper the same width as the content element

        document.querySelector(".footer-wrapper").style.width =
          document.querySelector(".content").getBoundingClientRect().width +
          "px";

        if (window.innerWidth > 990) {
          burger.classList.remove("burger-active");
          mainBlurr.style.display = "none";
          sideNav.style.transition = "width 0.2s";
          sideNav.style.width = "0";
          container.classList.remove("container-active");
          navDetails.classList.remove("nav-details-active");
          container.style.transition = "none";
          body.classList.remove("body-active");
          setTimeout(() => {
            container.style.transition = "transform 0.2s ease-out";
          }, 100);

          // remove the styling from the submenus when side nav removed

          const allSubMenus = [
            ...Array.from(document.querySelectorAll(".sub-menu")),
            ...Array.from(document.querySelectorAll(".sub-menu-2")),
          ];

          allSubMenus.forEach((menu) => {
            menu.classList.remove("sub-menu-active");
          });

          //REMOVE SEARCH-ACTIVE

          searchActive.style.display='none';
        }
      });

      //Eventlisteners for sub-nav catergories

      dropBoxes.forEach((dropBox, index) => {
        dropBox.addEventListener("click", (el) => {
          // Array.from(document.querySelectorAll('.sub-menu')).forEach((subMenu)=>{
          //   subMenu.classList.remove('sub-menu-active')
          // })

          // console.log(dropBoxes[1] === el.target)

          dropBoxes.forEach((dbox, index) => {
            if (dbox === el.target) {
              el.target.parentElement.children[2].classList.toggle(
                "sub-menu-active"
              );
            } else {
              dbox.parentElement.children[2].classList.remove(
                "sub-menu-active"
              );
            }
          });
        });
      });

      //Event Listeners for second sub menus

      secondDropBoxes.forEach((secondDropBox) => {
        secondDropBox.addEventListener("click", (el) => {
          secondDropBoxes.forEach((dbox) => {
            if (dbox === el.target) {
              el.target.parentElement.children[2].classList.toggle(
                "sub-menu-active"
              );
            } else {
              dbox.parentElement.children[2].classList.remove(
                "sub-menu-active"
              );
            }
          });
        });
      });

      // const submenu2= document.querySelector('.submenu2');
      // const subMenu =Array.from(document.querySelectorAll('.sub-menu'));

      // document.querySelector('.sub-menu').firstElementChild.children[0].children[1].style.height=subMenu[0].getBoundingClientRect().height + 'px';

      // document.querySelector('footer-details').style.width = document.querySelector('.content').style.width;

        document.querySelector(".footer-wrapper").style.width =
        document.querySelector(".content").getBoundingClientRect().width + "px";


        //Search button

        searchButton.addEventListener('click',()=>{
          searchActive.style.display='flex';
        })

        closeSearchButton.addEventListener('click',()=>{
          searchActive.style.display='none';
        })
        
    </script>
